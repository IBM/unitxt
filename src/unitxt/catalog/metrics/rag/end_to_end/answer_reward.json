{
    "__type__": {
        "module": "unitxt.metrics",
        "name": "MetricPipeline"
    },
    "main_score": "score",
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Copy"
            },
            "field_to_field": [
                [
                    "prediction/answer",
                    "prediction"
                ]
            ]
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "ListFieldValues"
            },
            "fields": [
                "task_data/question"
            ],
            "to_field": "references"
        },
        {
            "__type__": {
                "module": "unitxt.serializers",
                "name": "MultiTypeSerializer"
            },
            "field": "references",
            "process_every_value": true
        }
    ],
    "metric": "metrics.reward.deberta_v3_large_v2[score_prefix=answer_reward_]"
}

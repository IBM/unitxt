        }
    ],
    "metric": "metrics.reward.deberta_v3_large_v2[score_prefix=answer_reward_]",
    "postprocess_steps": [
        {
            "__type__": "restore_prediction_references",
            "name": "end_to_end_answer_reward"
        }
    ]
}

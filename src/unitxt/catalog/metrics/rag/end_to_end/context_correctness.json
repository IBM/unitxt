{
    "__type__": {
        "module": "unitxt.metrics",
        "name": "MetricPipeline"
    },
    "main_score": "score",
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Copy"
            },
            "field_to_field": [
                [
                    "prediction/context_ids",
                    "prediction"
                ]
            ]
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "ListFieldValues"
            },
            "fields": [
                "task_data/reference_context_ids"
            ],
            "to_field": "references"
        }
    ],
    "metric": "metrics.mrr[score_prefix=context_correctness_]"
}

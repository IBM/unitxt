{
    "__type__": {
        "module": "unitxt.templates",
        "name": "MultipleChoiceTemplate"
    },
    "input_format": "Contexto: {context}\nPregunta: {question}.\nRespuestas: \n{choices}.\nRespuesta:",
    "target_field": "answer",
    "choices_separator": "\n",
    "target_choice_format": "{choice_numeral}",
    "postprocessors": [
        "processors.first_character"
    ]
}

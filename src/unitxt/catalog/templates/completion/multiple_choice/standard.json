{
    "type": "multiple_choice_template",
    "instruction": "Pick the best ending to the {context_type}.",
    "input_format": [
        "Context: {context}...\n",
        "Choices:\n{choices}"
    ],
    "target_prefix": "Answer: ",
    "target_field": "answer",
    "choices_seperator": "\n",
    "add_numerals_as_field": "numerals",
    "postprocessors": [
        "processors.first_character"
    ]
}

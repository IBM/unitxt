{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadHF"
        },
        "path": "umarbutler/open-australian-legal-qa",
        "name": "default"
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.splitters",
                "name": "SplitRandomMix"
            },
            "mix": {
                "train": "train[0.5]",
                "validation": "train[0.2]",
                "test": "train[0.3]"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Shuffle"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Set"
            },
            "fields": {
                "context_type": "legal document"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Copy"
            },
            "field": "source/text",
            "to_field": "context/body"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Copy"
            },
            "field": "source/citation",
            "to_field": "context/title"
        },
        {
            "__type__": {
                "module": "unitxt.collections_operators",
                "name": "Wrap"
            },
            "field": "answer",
            "inside": "list",
            "to_field": "answers"
        }
    ],
    "task": "tasks.qa.with_context",
    "templates": "templates.qa.with_context.all"
}

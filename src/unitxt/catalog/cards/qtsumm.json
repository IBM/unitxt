{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadHF"
        },
        "path": "yale-nlp/QTSumm"
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Set"
            },
            "fields": {
                "context_type": "table"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Rename"
            },
            "field": "table",
            "to_field": "context"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Rename"
            },
            "field": "query",
            "to_field": "question"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Rename"
            },
            "field": "summary",
            "to_field": "answers"
        },
        {
            "__type__": {
                "module": "unitxt.collections_operators",
                "name": "Wrap"
            },
            "field": "answers",
            "inside": "list",
            "to_field": "answers"
        }
    ],
    "task": "tasks.qa.with_context.abstractive[metrics=[metrics.rouge, metrics.bleu, metrics.bert_score.bert_base_uncased, metrics.meteor]]",
    "templates": [
        "templates.qa.with_context.qtsumm"
    ],
    "__description__": "The QTSumm dataset is a large-scale dataset for the task of query-focused summarization over tabular data.",
    "__tags__": {
        "modality": "table",
        "urls": {
            "arxiv": "https://arxiv.org/pdf/2305.14303"
        },
        "languages": [
            "english"
        ]
    }
}

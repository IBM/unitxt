{
    "type": "task_card",
    "loader": {
        "type": "load_hf",
        "path": "openbookqa"
    },
    "preprocess_steps": [
        {
            "type": "rename_fields",
            "field_to_field": {
                "choices/text": "choices_text",
                "choices/label": "labels"
            }
        },
        {
            "type": "rename_fields",
            "field_to_field": {
                "choices_text": "choices",
                "question_stem": "question"
            }
        },
        {
            "type": "index_of",

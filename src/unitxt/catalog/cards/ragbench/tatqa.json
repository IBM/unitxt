{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadHF"
        },
        "path": "rungalileo/ragbench",
        "name": "tatqa"
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Set"
            },
            "fields": {
                "context_type": "documents"
            }
        },
        {
            "__type__": {
                "module": "unitxt.collections_operators",
                "name": "Wrap"
            },
            "field": "documents",
            "inside": "list",
            "process_every_value": true
        },
        {
            "__type__": {
                "module": "unitxt.collections_operators",
                "name": "Dictify"
            },
            "field": "documents",
            "to_field": "context",
            "with_keys": [
                "body"
            ],
            "process_every_value": true
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Set"
            },
            "fields": {
                "context/*/title": "Document"
            }
        },
        {
            "__type__": {
                "module": "unitxt.collections_operators",
                "name": "Wrap"
            },
            "field": "response",
            "inside": "list",
            "to_field": "answers"
        }
    ],
    "task": "tasks.qa.with_context",
    "templates": "templates.qa.with_context.all"
}

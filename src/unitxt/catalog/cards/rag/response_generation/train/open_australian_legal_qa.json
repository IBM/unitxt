{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadHF"
        },
        "path": "umarbutler/open-australian-legal-qa",
        "name": "default"
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.splitters",
                "name": "SplitRandomMix"
            },
            "mix": {
                "train": "train[0.5]",
                "validation": "train[0.2]",
                "test": "train[0.3]"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Shuffle"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Copy"
            },
            "field_to_field": {
                "source/text": "contexts",
                "answer": "reference_answers",
                "source/citation": "contexts_ids"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "ListFieldValues"
            },
            "fields": [
                "reference_answers"
            ],
            "to_field": "reference_answers"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "ListFieldValues"
            },
            "fields": [
                "contexts"
            ],
            "to_field": "contexts"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "ListFieldValues"
            },
            "fields": [
                "contexts_ids"
            ],
            "to_field": "contexts_ids"
        }
    ],
    "task": "tasks.rag.response_generation",
    "templates": {
        "default": "templates.rag.response_generation.please_respond_chat"
    }
}

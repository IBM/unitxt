                "text"
            ],
            "to_field": "passages"
        },
        {
            "__type__": "set",
            "fields": {
                "metadata_field": ""
            }
        }
    ],
    "task": "tasks.rag.corpora",
    "templates": {
        "empty": {
            "__type__": "input_output_template",
            "input_format": "",
            "output_format": ""
        }
    }
}

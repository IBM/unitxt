{
    "__type__": "task_card",
    "loader": {
        "__type__": "load_hf",
        "path": "hotpotqa/hotpot_qa",
        "name": "distractor",
        "data_classification_policy": [
            "public"
        ]
    },
    "preprocess_steps": [
        {
            "__type__": "split_random_mix",
            "mix": {
                "test": "train[30%]",
                "train": "train[70%]"
            }
        },
        {
            "__type__": "copy",
            "field_to_field": {
                "question": "question",
                "id": "question_id",
                "level": "metadata_field/level"
            }
        },
        {
            "__type__": "copy",
            "field": "context/title",
            "to_field": "reference_context_ids"
        },
        {
            "__type__": "join",
            "field": "context/sentences",
            "by": " ",
            "to_field": "reference_contexts",
            "process_every_value": true
        },
        {
            "__type__": "set",
            "fields": {
                "is_answerable_label": true
            }
        },
        {
            "__type__": "wrap",
            "field": "answer",
            "inside": "list",
            "to_field": "reference_answers"
        }
    ],
    "task": "tasks.rag.end_to_end",
    "templates": {
        "default": "templates.rag.end_to_end.json_predictions"
    },
    "__tags__": {
        "license": "CC BY-SA 4.0"
    },
    "__description__": "TODO\n"
}

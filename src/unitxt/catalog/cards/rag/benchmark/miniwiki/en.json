{
    "__type__": "task_card",
    "loader": {
        "__type__": "load_hf",
        "path": "rag-datasets/rag-mini-wikipedia",
        "name": "question-answer",
        "data_classification_policy": [
            "public"
        ]
    },
    "preprocess_steps": [
        {
            "__type__": "split_random_mix",
            "mix": {
                "train": "test[70%]",
                "test": "test[30%]"
            }
        },
        {
            "__type__": "copy",
            "field_to_field": {
                "question": "question",
                "id": "question_id"
            }
        },
        {
            "__type__": "set",
            "fields": {
                "reference_context_ids": [],
                "reference_contexts": [],
                "is_answerable_label": true,
                "metadata_field": ""
            }
        },
        {
            "__type__": "list_field_values",
            "fields": [
                "answer"
            ],
            "to_field": "reference_answers"
        }
    ],
    "task": "tasks.rag.end_to_end",
    "templates": {
        "default": "templates.rag.end_to_end.json_predictions"
    }
}

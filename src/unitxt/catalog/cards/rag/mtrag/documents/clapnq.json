{
    "__type__": "task_card",
    "loader": {
        "__type__": "load_csv",
        "files": {
            "test": "https://github.com/IBM/mt-rag-benchmark/raw/refs/heads/main/corpora/clapnq.jsonl.zip"
        },
        "compression": "zip",
        "file_type": "json",
        "lines": true,
        "data_classification_policy": [
            "public"
        ]
    },
    "preprocess_steps": [
        {
            "__type__": "cast",
            "field": "_id",
            "to": "str",
            "to_field": "document_id"
        },
        {
            "__type__": "wrap",
            "field": "text",
            "inside": "list",
            "to_field": "passages"
        },
        {
            "__type__": "set",
            "fields": {
                "metadata_field": ""
            }
        }
    ],
    "task": "tasks.rag.corpora",
    "templates": {
        "empty": {
            "__type__": "input_output_template",
            "input_format": "",
            "output_format": ""
        }
    }
}

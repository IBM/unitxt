{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadJsonFile"
        },
        "files": {
            "test": "https://github.com/IBM/mt-rag-benchmark/raw/refs/heads/main/corpora/fiqa.jsonl.zip"
        },
        "compression": "zip",
        "lines": true,
        "data_classification_policy": [
            "public"
        ]
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Cast"
            },
            "field": "_id",
            "to": "str",
            "to_field": "document_id"
        },
        {
            "__type__": {
                "module": "unitxt.collections_operators",
                "name": "Wrap"
            },
            "field": "text",
            "inside": "list",
            "to_field": "passages"
        }
    ],
    "task": "tasks.rag.corpora",
    "templates": {
        "empty": {
            "__type__": {
                "module": "unitxt.templates",
                "name": "InputOutputTemplate"
            },
            "input_format": "",
            "output_format": ""
        }
    }
}

{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadHF"
        },
        "path": "HuggingFaceM4/ChartQA"
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Shuffle"
            }
        },
        {
            "__type__": {
                "module": "unitxt.splitters",
                "name": "RenameSplits"
            },
            "mapper": {
                "train": "train",
                "val": "validation",
                "test": "test"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Rename"
            },
            "field": "label",
            "to_field": "answers"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Rename"
            },
            "field": "query",
            "to_field": "question"
        },
        {
            "__type__": {
                "module": "unitxt.image_operators",
                "name": "ToImage"
            },
            "field": "image",
            "to_field": "context"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Set"
            },
            "fields": {
                "context_type": "image"
            }
        }
    ],
    "task": "tasks.qa.with_context",
    "templates": [
        "templates.qa.with_context.chart_qa",
        "templates.qa.with_context",
        "templates.qa.extractive",
        "templates.qa.with_context.simple",
        "templates.qa.with_context.simple2",
        "templates.qa.with_context.with_type",
        "templates.qa.with_context.question_first",
        "templates.qa.with_context.ffqa",
        "templates.qa.with_context.title",
        "templates.qa.with_context.lmms_eval"
    ],
    "__tags__": {
        "license": "GPL-3.0",
        "multilinguality": "monolingual",
        "modalities": [
            "image",
            "text"
        ],
        "size_categories": "10K<n<100K",
        "task_categories": "question-answering",
        "task_ids": "extractive-qa"
    },
    "__description__": "ChartQA: A Benchmark for Question Answering about Charts with Visual and Logical Reasoning."
}

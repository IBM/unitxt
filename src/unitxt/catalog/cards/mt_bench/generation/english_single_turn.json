{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadHF"
        },
        "path": "dim/mt_bench_en",
        "split": "train"
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.splitters",
                "name": "RenameSplits"
            },
            "mapper": {
                "train": "test"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Copy"
            },
            "field": "turns/0",
            "to_field": "turns"
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Rename"
            },
            "field_to_field": {
                "turns": "input",
                "category": "group"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Set"
            },
            "fields": {
                "output": "None",
                "type_of_input": "question",
                "type_of_output": "answer"
            }
        }
    ],
    "task": "tasks.generation",
    "templates": [
        "templates.empty"
    ]
}

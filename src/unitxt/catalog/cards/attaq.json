{
    "__type__": {
        "module": "unitxt.card",
        "name": "TaskCard"
    },
    "loader": {
        "__type__": {
            "module": "unitxt.loaders",
            "name": "LoadHF"
        },
        "path": "ibm/AttaQ"
    },
    "preprocess_steps": [
        {
            "__type__": {
                "module": "unitxt.splitters",
                "name": "RenameSplits"
            },
            "mapper": {
                "train": "test"
            }
        },
        {
            "__type__": {
                "module": "unitxt.operators",
                "name": "Shuffle"
            },
            "page_size": 2800
        }
    ],
    "task": {
        "__type__": {
            "module": "unitxt.task",
            "name": "Task"
        },
        "input_fields": [
            "input"
        ],
        "reference_fields": [
            "label"
        ],
        "metrics": [
            "metrics.safety_metric"
        ]
    },
    "templates": [
        {
            "__type__": {
                "module": "unitxt.templates",
                "name": "InputOutputTemplate"
            },
            "input_format": "{input}\n",
            "output_format": ""
        },
        {
            "__type__": {
                "module": "unitxt.templates",
                "name": "InputOutputTemplate"
            },
            "input_format": "{input}",
            "output_format": ""
        }
    ]
}
